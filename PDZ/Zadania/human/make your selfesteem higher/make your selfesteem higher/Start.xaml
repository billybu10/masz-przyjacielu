        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:make_your_selfesteem_higher"
        mc:Ignorable="d"
        Title="Start" Height="450" Width="800">
    <Grid Background="Black">
