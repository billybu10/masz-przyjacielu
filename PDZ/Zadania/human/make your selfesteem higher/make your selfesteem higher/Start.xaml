        <Label x:Name="pluce" Content="Płuce" HorizontalAlignment="Left" Height="49" Margin="437,227,0,0" VerticalAlignment="Top" Width="70" Foreground="White" FontFamily="Segoe UI Black" FontSize="22"/>
        <Label x:Name="nerke" Content="Nerke" HorizontalAlignment="Left" Height="46" Margin="437,276,0,0" VerticalAlignment="Top" Width="83" Foreground="White" FontSize="22" FontFamily="Segoe UI Black"/>
        <TextBox x:Name="cena_muzge" HorizontalAlignment="Left" Height="46" Margin="559,176,0,0" TextWrapping="Wrap" Text="58100" VerticalAlignment="Top" Width="166" FontFamily="Segoe UI Black" FontSize="24" Background="Black" Foreground="White" BorderBrush="Black"/>
        <Label x:Name="dolar1" Content="$" HorizontalAlignment="Left" Height="46" Margin="730,176,0,0" VerticalAlignment="Top" Width="40" Foreground="White" FontSize="22" FontFamily="Segoe UI Black"/>
        <TextBox x:Name="cena_nerke" HorizontalAlignment="Left" Height="46" Margin="559,276,0,0" TextWrapping="Wrap" Text="58100" VerticalAlignment="Top" Width="166" FontFamily="Segoe UI Black" FontSize="24" Background="Black" Foreground="White" BorderBrush="Black"/>
        <Label x:Name="dolar3" Content="$" HorizontalAlignment="Left" Height="46" Margin="730,276,0,0" VerticalAlignment="Top" Width="40" Foreground="White" FontSize="22" FontFamily="Segoe UI Black"/>
        <TextBox x:Name="cena_pluce" HorizontalAlignment="Left" Height="46" Margin="559,227,0,0" TextWrapping="Wrap" Text="58100" VerticalAlignment="Top" Width="166" FontFamily="Segoe UI Black" FontSize="24" Background="Black" Foreground="White" BorderBrush="Black" TextChanged="Cena_muzge_Copy1_TextChanged"/>
        <Label x:Name="dolar2" Content="$" HorizontalAlignment="Left" Height="46" Margin="730,227,0,0" VerticalAlignment="Top" Width="40" Foreground="White" FontSize="22" FontFamily="Segoe UI Black"/>
        <Button x:Name="button" Content="Start" HorizontalAlignment="Left" Margin="513,327,0,0" VerticalAlignment="Top" Width="154" Height="64" Foreground="White" FontFamily="Segoe UI Black" FontSize="24" IsCancel="True" Click="Button_Click">
            <Button.Background>
                <RadialGradientBrush>
                    <GradientStop Color="Black" Offset="1"/>
                    <GradientStop Color="White"/>
                </RadialGradientBrush>
            </Button.Background>
        </Button>

